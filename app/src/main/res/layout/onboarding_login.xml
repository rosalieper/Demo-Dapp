<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="449dp"
        android:layout_height="380dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/Ellipse"

        tools:layout_editor_absoluteX="-20dp"
        tools:layout_editor_absoluteY="-20dp" />

    <View
        android:id="@+id/ellipse_2"
        android:layout_width="184dp"
        android:layout_height="183dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/ellipse_2"
        tools:layout_editor_absoluteX="113dp"
        tools:layout_editor_absoluteY="149dp" />

    <LinearLayout
        android:layout_width="345dp"
        android:layout_height="636dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="47dp">

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="29dp"
        android:layout_height="26dp"
        app:srcCompat="@drawable/show_pss"
        tools:layout_editor_absoluteX="304dp"
        tools:layout_editor_absoluteY="423dp" />

    <View
        android:id="@+id/group"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:background="@drawable/Groupsignin"
        android:elevation="4dp"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="529dp" />

    <TextView
        android:id="@+id/skip"
        android:layout_width="100dp"
        android:layout_height="19dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center_horizontal"
        android:text="@string/create_account"
        android:textAppearance="@style/skip"
        app:layout_constraintTop_toBottomOf="@+id/group"
        tools:layout_editor_absoluteX="155dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="116dp"
        android:layout_height="115dp"
        app:srcCompat="@drawable/logonzinghaa"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="192dp" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="253dp"
        android:layout_height="54dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="423dp" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="255dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="369dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/password"
        tools:layout_editor_absoluteX="172dp"
        tools:layout_editor_absoluteY="449dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UserName"
        tools:layout_editor_absoluteX="172dp"
        tools:layout_editor_absoluteY="384dp" />
</android.support.constraint.ConstraintLayout>