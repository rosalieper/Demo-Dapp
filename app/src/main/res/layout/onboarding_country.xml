<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="449dp"
        android:layout_height="380dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/Ellipse"

        tools:layout_editor_absoluteX="-20dp"
        tools:layout_editor_absoluteY="-20dp" />

    <View
        android:id="@+id/ellipse_2"
        android:layout_width="184dp"
        android:layout_height="183dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/ellipse_2"
        tools:layout_editor_absoluteX="113dp"
        tools:layout_editor_absoluteY="149dp" />

    <LinearLayout
        android:layout_width="345dp"
        android:layout_height="636dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="47dp">


    </LinearLayout>
    <EditText
        android:id="@+id/editText2"
        android:layout_width="278dp"
        android:layout_height="38dp"
        android:textAlignment="center"
        android:ems="10"
        android:text="@string/country"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="460dp" />

    <Spinner
        android:id="@+id/planets_spinner"
        android:layout_width="280dp"
        android:layout_height="42dp"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="456dp" />

    <TextView
        android:id="@+id/now_to_give"
        android:layout_width="285dp"
        android:layout_height="56dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center_horizontal"
        android:text="@string/now_to_give"
        android:textAppearance="@style/now_to_give"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="393dp" />


    <View
        android:id="@+id/group"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:background="@drawable/group"
        android:elevation="4dp"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="532dp" />



    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/safeboxcountry"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="191dp" />

</android.support.constraint.ConstraintLayout>